{
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --colors --config resources/build/webpack.development.js",
    "hot": "cross-env NODE_ENV=hot webpack-dev-server --colors --config resources/build/webpack.development.js",
    "build": "cross-env WPEMERGE_COMBINED_BUILD=1 run-s -l build:debug build:production",
    "build:debug": "cross-env NODE_ENV=debug webpack --colors --config resources/build/webpack.production.js",
    "build:production": "cross-env NODE_ENV=production webpack --colors --config resources/build/webpack.production.js",
    "lint": "run-s -cl lint:wpcs lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "lint:wpcs": "./vendor/bin/phpcs --colors --standard=phpcs.xml ./",
    "lint:scripts": "eslint --color ./resources/scripts/**",
    "lint:styles": "stylelint --color ./resources/styles/**",
    "lint-fix:wpcs": "./vendor/bin/phpcbf --colors --standard=wpcs.xml ./",
    "lint-fix:scripts": "eslint --color --fix ./resources/scripts/**",
    "lint-fix:styles": "stylelint --color --fix ./resources/styles/**",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=dist,node_modules,resources,tests,vendor --textdomain=anymarket",
    "i18n:pot": "wpi18n makepot --exclude=dist,node_modules,resources,tests,vendor --type=wp-plugin --domain-path=languages/ --pot-file=anymarket.pot --textdomain=anymarket",
    "rebrand": "node resources/build/rebrand/rebrand.js",
    "release": "run-s build release:zip",
    "release:zip": "node resources/build/release/release.js woocommerce-anymarket-release"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "clipboard": "^2.0.6",
    "typeface-roboto": "^0.0.75",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-content-loader": "^0.2.3",
    "vue-good-table": "^2.20.0",
    "vue-js-toggle-button": "^1.3.3",
    "vue-loading-overlay": "^3.3.2",
    "vue-router": "^3.3.4",
    "vue-toasted": "^1.1.28",
    "vue-wp-list-table": "^1.2.0",
    "vuetify": "^2.3.4",
    "wysiwyg.css": "^0.0.3"
  },
  "devDependencies": {
    "@babel/core": "~7.2",
    "@babel/plugin-proposal-class-properties": "~7.3",
    "@babel/plugin-proposal-json-strings": "~7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "~7.2",
    "@babel/plugin-syntax-import-meta": "~7.2",
    "@babel/preset-env": "~7.3",
    "airbnb-browser-shims": "^3.3.0",
    "archiver": "^4.0.1",
    "autoprefixer": "^9.8.0",
    "babel-loader": "~8.0",
    "chalk": "^4.0.0",
    "change-case": "^4.1.1",
    "chokidar": "^3.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "glob": "^7.1.4",
    "html-loader": "^1.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "import-glob": "^1.5.0",
    "inquirer": "^7.1.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.15",
    "markdown-loader": "^5.1.0",
    "marked": "^1.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "node-wp-i18n": "^1.2.1",
    "npm-run-all": "^4.1.3",
    "postcss-discard-comments": "^4.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "prettier": "^2.0.5",
